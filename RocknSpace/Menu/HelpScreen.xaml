<UserControl x:Class="RocknSpace.Menu.HelpScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RocknSpace.Menu"
             mc:Ignorable="d" 
             d:DesignHeight="700" d:DesignWidth="1000">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Image x:Name="image" Source="/RocknSpace;component/Images/help.jpg" Stretch="Fill"/>
        <ContentControl Style="{StaticResource MenuBackground}" Height="200" Width="322" MinHeight="100" MinWidth="100">
            <Grid>
                <Button Style="{StaticResource MenuButton}" Content="gOt iT" Click="Back_Click" Margin="53,68,46,-68" Width="223"/>
            </Grid>
        </ContentControl>
    </Grid>

</UserControl>